{
	"root": "apps/app",
	"projectType": "application",
	"sourceRoot": "apps/app/src",
	"targets": {
		"build": {
			"executor": "nx-vue3-vite:build-app",
			"options": {
				"dist": "dist/apps/app"
			}
		},
		"serve": {
			"executor": "nx-vue3-vite:dev-server",
			"options": {
				"proxyConfig": "apps/app/proxy.conf.json"
			}
		},
		"e2e": {
			"executor": "nx-vue3-vite:cypress",
			"options": {
				"cypressConfig": "apps/app/cypress.json",
				"testingType": "component"
			}
		},
		"lint": {
			"executor": "@nrwl/linter:eslint",
			"options": {
				"lintFilePatterns": ["apps/app/**/*.{js,jsx,ts,tsx,vue}"]
			}
		},
		"test": {
			"executor": "@nrwl/jest:jest",
			"outputs": ["coverage/apps/app"],
			"options": {
				"jestConfig": "apps/app/jest.config.js",
				"passWithNoTests": true
			}
		}
	},
	"tags": []
}
